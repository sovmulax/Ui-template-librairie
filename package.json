{
  "name": "ui-template-librairie",
  "version": "1.0.0",
  "description": "Template de librairie en ligne avec TailwindCSS",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "css:dev": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "css:build": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --minify"
  },
  "keywords": [
    "tailwindcss",
    "librairie",
    "template"
  ],
  "author": "Votre nom",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.0",
    "vite": "^7.1.7"
  }
}
